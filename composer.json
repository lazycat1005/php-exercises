{
  "name": "php-exercises/project",
  "description": "PHP 練習題專案 - 用於練習 PHP 7.3 與 jQuery 1.12.4",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "PHP Exercises",
      "email": "php-exercises@example.com"
    }
  ],
  "require": {
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "config/exercises.php"
